{
  "name": "mart-for-you",
  "version": "0.0.1",
  "description": "A modern, full-featured e-commerce web application built with React and Tailwind CSS. Features product browsing, filtering, cart management, and user profile functionality.",
  "private": true,
  "type": "module",
  "license": "MIT",
  "keywords": [
    "react",
    "ecommerce",
    "shopping-cart",
    "tailwindcss",
    "vite",
    "framer-motion",
    "dark-mode",
    "responsive",
    "spa",
    "frontend"
  ],
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ethara-AI/mart-for-you.git"
  },
  "bugs": {
    "url": "https://github.com/Ethara-AI/mart-for-you/issues"
  },
  "homepage": "https://components.ethara.ai/mart-for-you",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "clean": "rimraf dist node_modules/.cache",
    "clean:all": "rimraf dist node_modules",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "framer-motion": "^12.24.12",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "gh-pages": "^6.3.0",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  }
}
